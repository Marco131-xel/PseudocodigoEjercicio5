Algoritmo Ejercicio5
 // donde M es mujer y H es hombre 
 M <- >18
 H <- <18
 Para i <- 1 Hasta E Con Paso 1
  Hacer
  Definir M,H,E
  // E es edad
  Escribir"Escribi aqui si eres mujer"
   Leer M
  Escribir"Escribi aqui si ere hombre"
   Leer H
  Escribir"Escribi aqui tu edad"
   Leer E
  Si M Entonces
  M <- E
  Sino
  H <- E
 Fin Para
 Escribir"Las mujeres de mayor de edad es M"
 Escribir"Los hombres de menor de edad es H"
FinAlgoritmo 
